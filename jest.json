{
  "moduleFileExtensions": ["ts", "tsx", "js", "json"],
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "testRegex": "/test/.*\\.(test|e2e-spec).(ts|tsx|js)$",
  "collectCoverageFrom": [
    "**/*.{js,jsx}",
    "!**/node_modules/**",
    "!**/vendor/**"
  ],
  "coverageReporters": ["clover", "json", "lcov", "text"]
}
